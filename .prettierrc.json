{
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "tailwindFunctions": [
    "classNames",
    "cn"
  ],
  "arrowParens": "always",
  "singleQuote": false,
  "proseWrap": "always",
  "printWidth": 80,
  "endOfLine": "lf",
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "semi": true,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "singleAttributePerLine": true
}
